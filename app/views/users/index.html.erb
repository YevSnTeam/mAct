<% if signed_in? %>
	<% if @users.any? %>
		<ul>
			<% @users.each do |user| %>
			<li>
				<%= link_to user.fullname, user %>
				<% if current_user != user %>
					<%= render :partial => "handle_friendslinks", :locals => { :user => user } %>
				<% else %>	
					 das bist du 
				<% end %>	
			</li>
			<%end%>
		</ul>
	<% else %>
		<p>Komischerweise gibt es noch keine Users</p>
	<% end %>
<% else %>
	<%= link_to "Login", login_path %>
	<%= link_to "Sign UP", signup_path %>
<% end %>
 